{
  "jest": {
    "verbose": true,
    "testTimeout": 5000,
    "collectCoverage": false,
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.mjs$": "babel-jest"
    },
    "testRegex": "((\\.|/*.)(should))\\.js?$"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "bulma": "^0.9.4",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1"
  }
}
